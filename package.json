{
	"name": "german-synonyms",
	"displayName": "German Synonyms",
	"description": "Extension to easily lookup synonyms while writing text in german",
	"version": "1.2.0",
	"publisher": "Tim-Koehler",
	"repository": {
		"url": "https://github.com/tim-koehler/German-Synonyms"
	},
	"engines": {
		"vscode": "^1.49.0"
	},
	"keywords": [
		"German",
		"Synonyms",
		"Language",
		"German-Synonyms"
	],
	"icon": "images/icon.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:German-Synonyms.lookupSynonym"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "German-Synonyms.lookupSynonym",
				"title": "German-Synonyms: Lookup Synonym"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
